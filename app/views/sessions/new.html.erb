<%=render partial: "pages/anonymous-nav-bar" %>

<div class="boxed2">  
  <h1>Log in</h1>
  <div class='form'>
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label      :username %>
    <%= f.text_field :username %>

    <%= f.label          :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log in" %>
  <% end %>
  </div>
  <p>New user? <%= link_to "Sign up now!", signup_path %></p>
</div>

